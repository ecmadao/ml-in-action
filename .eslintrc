{
  "extends": "eslint-config-airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "no-multi-spaces": 0,
    "no-param-reassign": ["error", { "props": false }],
    "class-methods-use-this": 0,
    "camelcase": 0,
    "no-continue": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "no-return-await": 0,
    "no-await-in-loop": 0,
    "no-restricted-properties": 0
  },
  "globals": {
    "require": true
  }
}
